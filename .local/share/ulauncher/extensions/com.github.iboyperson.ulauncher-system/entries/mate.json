{
    "lock": {
        "command": "mate-screensaver-command --lock"
    },
    "log-out": {
        "command": "mate-session-save --logout-dialog"
    },
    "suspend": {
        "command": "sh -c 'mate-screensaver-command --lock && systemctl suspend -i'"
    },
    "hibernate": {
        "command": "sh -c 'mate-screensaver-command --lock && systemctl hibernate -i'"
    },
    "restart": {
        "command": "mate-session-save --shutdown-dialog"
    },
    "shutdown": {
        "command": "mate-session-save --shutdown-dialog"
    }
}
