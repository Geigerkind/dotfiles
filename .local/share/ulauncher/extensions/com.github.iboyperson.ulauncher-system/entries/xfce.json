{
    "lock": {
        "command": "xflock4"
    },
    "log-out": {
        "command": "xfce4-session-logout --logout"
    },
    "suspend": {
        "command": "xfce4-session-logout --suspend"
    },
    "hibernate": {
        "command": "xfce4-session-logout --hibernate"
    },
    "restart": {
        "command": "xfce4-session-logout --reboot"
    },
    "shutdown": {
        "command": "xfce4-session-logout --halt"
    }
}
